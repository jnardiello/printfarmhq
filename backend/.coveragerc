[run]
source = app
omit = 
    */tmp/*
    */temp/*
    */__pycache__/*
    */tests/*
    */migrations/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    
[html]
directory = htmlcov

[xml]
output = coverage.xml